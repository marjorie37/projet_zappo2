{"code":"import { listen } from 'popmotion';\r\nvar makeDraggable = function (element, activeActions, setPose, _a) {\r\n    var onDragStart = _a.onDragStart, onDragEnd = _a.onDragEnd;\r\n    var dragStartListener = listen(element, 'mousedown touchstart').start(function (startEvent) {\r\n        startEvent.preventDefault();\r\n        setPose('dragging');\r\n        if (onDragStart)\r\n            onDragStart(startEvent);\r\n        var dragEndListener = listen(document, 'mouseup touchend').start(function (endEvent) {\r\n            activeActions.get('dragEndListener').stop();\r\n            setPose('dragEnd');\r\n            if (onDragEnd)\r\n                onDragEnd(endEvent);\r\n        });\r\n        activeActions.set('dragEndListener', dragEndListener);\r\n    });\r\n    activeActions.set('dragStartListener', dragStartListener);\r\n};\r\nexport default (function (element, activeActions, setPose, _a) {\r\n    var props = _a.props;\r\n    if (props.draggable)\r\n        makeDraggable(element, activeActions, setPose, props);\r\n});\r\n//# sourceMappingURL=events.js.map","map":"{\"version\":3,\"file\":\"events.js\",\"sourceRoot\":\"\",\"sources\":[\"../src/dom/events.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,MAAM,EAAoB,MAAM,WAAW,CAAC;AAMrD,IAAM,aAAa,GAAG,UACpB,OAAgB,EAChB,aAA8C,EAC9C,OAAe,EACf,EAA8C;QAA5C,4BAAW,EAAE,wBAAS;IAExB,IAAM,iBAAiB,GAAG,MAAM,CAAC,OAAO,EAAE,sBAAsB,CAAC,CAAC,KAAK,CACrE,UAAC,UAAmC;QAClC,UAAU,CAAC,cAAc,EAAE,CAAC;QAC5B,OAAO,CAAC,UAAU,CAAC,CAAC;QAEpB,IAAI,WAAW;YAAE,WAAW,CAAC,UAAU,CAAC,CAAC;QAEzC,IAAM,eAAe,GAAG,MAAM,CAAC,QAAQ,EAAE,kBAAkB,CAAC,CAAC,KAAK,CAChE,UAAC,QAAiC;YAChC,aAAa,CAAC,GAAG,CAAC,iBAAiB,CAAC,CAAC,IAAI,EAAE,CAAC;YAC5C,OAAO,CAAC,SAAS,CAAC,CAAC;YACnB,IAAI,SAAS;gBAAE,SAAS,CAAC,QAAQ,CAAC,CAAC;QACrC,CAAC,CACF,CAAC;QAEF,aAAa,CAAC,GAAG,CAAC,iBAAiB,EAAE,eAAe,CAAC,CAAC;IACxD,CAAC,CACF,CAAC;IAEF,aAAa,CAAC,GAAG,CAAC,mBAAmB,EAAE,iBAAiB,CAAC,CAAC;AAC5D,CAAC,CAAC;AAEF,gBAAe,UACb,OAAgB,EAChB,aAA8C,EAC9C,OAAe,EACf,EAA6B;QAA3B,gBAAK;IAEP,IAAI,